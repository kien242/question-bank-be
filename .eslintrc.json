{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["google"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": ["error", "always"],
        "new-cap": ["error", {"capIsNewExceptions": ["Router"]}],
        "space-before-function-paren":"off"
    }
}
